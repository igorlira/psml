      <para>Accents <xref frag="media" type="math">
        <media-fragment id="media" mediatype="application/mathml+xml">
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <mstyle displaystyle="true">
              <mover>
                <mi>x</mi>
                <mo>^</mo>
              </mover>
              <mover>
                <mi>x</mi>
                <mo>&#175;</mo>
              </mover>
              <munder>
                <mi>x</mi>
                <mo>&#818;</mo>
              </munder>
              <mover>
                <mi>x</mi>
                <mo stretchy="false">&#8594;</mo>
              </mover>
              <mover>
                <mi>x</mi>
                <mo>.</mo>
              </mover>
              <mover>
                <mi>x</mi>
                <mo>..</mo>
              </mover>
            </mstyle>
          </math>
        </media-fragment>
      </xref></para>
      <para>Font commands <xref frag="media" type="math">
