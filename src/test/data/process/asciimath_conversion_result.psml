    <fragment id="1-2">
      <para><xref frag="media" type="math">
        <media-fragment id="media" mediatype="application/mathml+xml">
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <mstyle displaystyle="true">
              <mrow>
                <msubsup>
                  <mo>&#8747;</mo>
                  <mrow>
                    <mo>-</mo>
                    <mn>1</mn>
                  </mrow>
                  <mn>1</mn>
                </msubsup>
              </mrow>
              <msqrt>
                <mrow>
                  <mn>1</mn>
                  <mo>-</mo>
                  <msup>
                    <mi>x</mi>
                    <mn>2</mn>
                  </msup>
                </mrow>
              </msqrt>
              <mrow>
                <mi>d</mi>
                <mi>x</mi>
              </mrow>
              <mo>=</mo>
              <mfrac>
                <mi>&#960;</mi>
                <mn>2</mn>
              </mfrac>
            </mstyle>
          </math>
        </media-fragment>
      </xref></para>
      <para><xref frag="media" type="math">
        <media-fragment id="media" mediatype="application/mathml+xml">
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <mstyle displaystyle="true">
              <msup>
                <mrow>
                  <mo>(</mo>
                  <mtable columnlines="none none">
                    <mtr>
                      <mtd>
                        <mi>a</mi>
                      </mtd>
                      <mtd>
                        <mi>b</mi>
                      </mtd>
                    </mtr>
                    <mtr>
                      <mtd>
                        <mi>c</mi>
                      </mtd>
                      <mtd>
                        <mi>d</mi>
                      </mtd>
                    </mtr>
                  </mtable>
                  <mo>)</mo>
                </mrow>
                <mrow>
                  <mo>-</mo>
                  <mn>1</mn>
                </mrow>
              </msup>
              <mo>=</mo>
              <mfrac>
                <mn>1</mn>
                <mrow>
                  <mi>a</mi>
                  <mi>d</mi>
                  <mo>-</mo>
                  <mi>b</mi>
                  <mi>c</mi>
                </mrow>
              </mfrac>
              <mrow>
                <mo>(</mo>
                <mtable columnlines="none none">
                  <mtr>
                    <mtd>
                      <mi>d</mi>
                    </mtd>
                    <mtd>
                      <mo>-</mo>
                      <mi>b</mi>
                    </mtd>
                  </mtr>
                  <mtr>
                    <mtd>
                      <mo>-</mo>
                      <mi>c</mi>
                    </mtd>
                    <mtd>
                      <mi>a</mi>
                    </mtd>
                  </mtr>
                </mtable>
                <mo>)</mo>
              </mrow>
            </mstyle>
          </math>
        </media-fragment>
      </xref></para>
