      <scheme level="8" type="upperroman" format="[(8)]" />
      <scheme level="9" type="loweralpha" format="[9]" />
    </schemes>
  </numbering>
  <numbering strip-zeros="false" label="autonumber2">
    <schemes>
      <scheme level="1" type="upperalpha" format="[1.]" />
